<wpml-config>
  <custom-types>
    <custom-type translate="1">staticblocks</custom-type>
    <custom-type translate="1">etheme_portfolio</custom-type>
    <custom-type translate="1">testimonials</custom-type>
  </custom-types>
  <custom-fields>
    <custom-field action="translate" style="line" label="Custom tab title">_et_custom_tab1_title</custom-field>
    <custom-field action="translate" style="textarea" label="Custom tab">_et_custom_tab1</custom-field>
    <custom-field action="copy-once" style="line" label="Sidebar position">_et_sidebar_state</custom-field>
    <custom-field action="copy-once" style="line" label="Sidebar width">_et_sidebar_width</custom-field>
    <custom-field action="copy-once" style="line" label="Breadcrumbs Style">_et_breadcrumb_type</custom-field>
    <custom-field action="copy-once" style="line" label="Breadcrumbs Effect">_et_breadcrumb_effect</custom-field>
    <custom-field action="copy-once" style="line" label="Fixed footer">_et_footer_fixed</custom-field>
    <custom-field action="copy-once" style="line" label="Disable copyrights">_et_remove_copyrights</custom-field>
    <custom-field action="copy-once" style="line" label="Custom background image">_et_bg_image</custom-field>
    <custom-field action="copy-once" style="line" label="Custom background color">_et_bg_color</custom-field>
  </custom-fields>
  <taxonomies>
    <taxonomy translate="1">portfolio_category</taxonomy>
    <taxonomy translate="1">testimonial-category</taxonomy>
  </taxonomies>
  <admin-texts>
    <key name="theme_mods_xstore">
      <key name="404_text"/>
      <key name="product_bage_banner"/>
      <key name="empty_cart_content"/>
      <key name="custom_tab_title"/>
      <key name="custom_tab"/>
      <key name="promo-link-text"/>
      <key name="pp_content"/>
      <key name="header_custom_block"/>
      <key name="all_departments_text"/>
      <key name="sale_icon_text"/>
      <key name="cart_popup_banner"/>
      <key name="product_socials_label_text_et-desktop"/>
      <key name="product_tabs_custom_tab_01_title_et-desktop"/>
      <key name="product_tabs_custom_tab_02_title_et-desktop"/>
      <key name="mobile_menu_text"/>
      <key name="mobile_menu_tab_2_text"/>
      <key name="cart_footer_content_et-desktop"/>
      <key name="booster_progress_content_et-desktop"/>
      <key name="booster_progress_content_success_et-desktop"/>
      <key name="account_text"/>
      <key name="account_logged_in_text"/>
      <key name="search_all_categories_text_et-desktop"/>
      <key name="search_placeholder_et-desktop"/>
      <key name="contacts_package_et-desktop"/>
      <key name="newsletter_label_et-desktop"/>
      <key name="newsletter_title_et-desktop"/>
      <key name="newsletter_content_et-desktop"/>
      <key name="button_text_et-desktop"/>
      <key name="promo_text_package"/>
      <key name="html_block1"/>
      <key name="html_block2"/>
      <key name="html_block3"/>

      <key name="product_size_guide_label_et-desktop"></key>
      <key name="product_size_guide_title_et-desktop"></key>
    </key>
  </admin-texts>
  <shortcodes>
    <shortcode>
      <tag>et_tab</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>banner</tag>
      <attributes>
          <attribute>title</attribute>
          <attribute>subtitle</attribute>
          <attribute type="link" encoding="vc_link">link</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>title</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
          <attribute encoding="allow_html_tags">subtitle</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>icon_box</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>team_member</tag>
    </shortcode>
    <shortcode>
      <tag>twitter</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>instagram</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
          <attribute encoding="allow_html_tags">link</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>etheme_categories</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>etheme_products</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>et_the_look</tag>
    </shortcode>
    <shortcode>
      <tag>menu</tag>
      <attributes>
        <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>et_menu_list</tag>
      <attributes>
        <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>et_menu_list_item</tag>
      <attributes>
        <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
  </shortcodes>
  <elementor-widgets>
    <widget name="etheme_categories_lists">
      <fields>
        <field type="etheme_categories_lists: title" editor_type="LINE">title</field>
      </fields>
    </widget>
    <widget name="testimonials">
      <fields-in-item items_of="testimonials_tab">
        <field type="testimonials: Name" editor_type="LINE">name</field>
        <field type="testimonials: Country" editor_type="LINE">country</field>
        <field type="testimonials: content" editor_type="VISUAL">content</field>
      </fields-in-item>
    </widget>
    <widget name="et-advanced-tabs">
      <fields-in-item items_of="et_tabs_tab">
        <field type="et-advanced-tabs: et_tabs_tab_title" editor_type="LINE">et_tabs_tab_title</field>
        <field type="et-advanced-tabs: et_tabs_content_title" editor_type="LINE">et_tabs_content_title</field>
      </fields-in-item>
    </widget>
    <widget name="et-general-tabs">
      <fields-in-item items_of="et_tabs_tab">
        <field type="et-general-tabs: et_tabs_tab_title" editor_type="LINE">et_tabs_tab_title</field>
        <field type="et-general-tabs: et_tabs_tab_title" editor_type="LINE">et_tabs_tab_title</field>
        <field type="et-general-tabs: staticblock" editor_type="LINE">staticblock</field>
      </fields-in-item>
    </widget>
    <widget name="team_member">
      <fields>
        <field type="team_member: name" editor_type="LINE">name</field>
        <field type="team_member: email" editor_type="LINE">email</field>
        <field type="team_member: position" editor_type="LINE">position</field>
        <field type="team_member: content" editor_type="VISUAL">content</field>
      </fields>
    </widget>
    <widget name="etheme_slider">
      <fields-in-item items_of="get_slider_item">
        <field type="etheme_slider: title" editor_type="LINE">title</field>
        <field type="etheme_slider: subtitle" editor_type="LINE">subtitle</field>
        <field type="etheme_slider: content" editor_type="VISUAL">content</field>
        <field type="etheme_slider: button_title" editor_type="LINE">button_title</field>
        <field type="etheme_slider: button_link" editor_type="LINK" key_of="button_link">url</field>
      </fields-in-item>
    </widget>
    <widget name="etheme_product_menu_layout">
      <fields>
        <field type="etheme_product_menu_layout: title" editor_type="LINE">title</field>
      </fields>
      <fields-in-item items_of="custom_content_tab">
        <field type="etheme_product_menu_layout: name" editor_type="LINE">name</field>
        <field type="etheme_product_menu_layout: categories" editor_type="LINE">categories</field>
        <field type="etheme_product_menu_layout: excerpt" editor_type="LINE">excerpt</field>
        <field type="etheme_product_menu_layout: price" editor_type="LINE">price</field>
      </fields-in-item>
    </widget>
    <widget name="etheme_products">
      <fields>
        <field type="etheme_products: title" editor_type="LINE">title</field>
      </fields>
    </widget>
    <widget name="et_menu_list">
      <fields>
        <field type="et_menu_list: title" editor_type="LINE">title</field>
        <field type="et_menu_list: link" editor_type="LINK" key_of="link">url</field>
      </fields>
      <fields-in-item items_of="menu_list_item">
        <field type="et_menu_list: title" editor_type="LINE">title</field>
        <field type="et_menu_list: link" editor_type="LINK" key_of="link">url</field>
      </fields-in-item>
    </widget>
    <widget name="etheme-instagram">
      <fields>
        <field type="etheme-instagram: title" editor_type="LINE">title</field>
        <field type="etheme-instagram: link" editor_type="LINK" key_of="link">url</field>
      </fields>
    </widget>
    <widget name="etheme_categories">
      <fields>
        <field type="etheme_categories: title" editor_type="LINE">title</field>
      </fields>
    </widget>
    <widget name="banner">
      <fields>
        <field type="banner: Title" editor_type="LINE">title</field>
        <field type="banner: Subtitle" editor_type="LINE">subtitle</field>
        <field type="banner: Content" editor_type="AREA">content</field>
        <field type="banner: link" editor_type="LINK" key_of="link">url</field>
        <field type="banner: button_title" editor_type="LINE">button_title</field>
        <field type="banner: button_link" editor_type="LINK" key_of="button_link">url</field>
      </fields>
    </widget>
  </elementor-widgets>
</wpml-config>